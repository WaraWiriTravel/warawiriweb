<!-- Pemanggilan bagian sidebar -->
<%- include("sidebar") %>
<!-- Begin Page Content -->

<div class="container">
  <div class="row mb-3 justify-content-center">
    <div class="d-sm-flex align-items-center justify-content-between mb-4">
      <h3 class="mb-0 mt-5">
        Selamat Datang Pada Panel Administrator Wara Wiri Tour
      </h3>
    </div>
  </div>
  <div class="row justify-content-center">
    <% highlightBlogs.forEach(blog => { %>
    <div class="col-md-4 mb-3">
      <a
        href="/blog-detail-<%= blog.title %>"
        class="text-reset text-decoration-none"
      >
        <div class="card">
          <img
            src="<%= blog.imageUrl %>"
            class="card-img-top img-home-highlight"
          />
          <div class="card-body">
            <p class="card-text"><%= blog.title %></p>
          </div>
        </div></a
      >
    </div>
    <% }); %>
  </div>
</div>
