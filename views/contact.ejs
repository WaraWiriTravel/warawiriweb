<!-- Pemanggilan bagian header -->
<%- include("partials/header") %>
<!-- Pemanggilan bagian Navbar -->
<%- include("partials/navbar") %>

<body id="body">
  <!-- Contact Us -->

  <div class="text-center mb-4 mt-4">
    <h2>Hubungi Kami</h2>
    <h5>Anda dapat menghubungi kami untuk informasi lebih lanjut.</h5>
  </div>

  <section>
    <div class="container mt-10">
      <div class="row justify-content-center">
        <div class="col-md-6">
          <div class="row">
            <div class="col-md-3">
              <div class="icon">
                <i class="fa fa-envelope fa-5x" aria-hidden="true"></i>
              </div>
            </div>

            <div class="col-md-6">
              <h4>Email</h4>
              <a href="mailto:warawiribusiness@gmail.com" style="color: black"
                ><%= kontak.email %></a
              >
            </div>
          </div>
          <div class="row">
            <div class="col-md-3">
              <i class="fa fa-whatsapp fa-5x" aria-hidden="true"></i>
            </div>
            <div class="col-md-9">
              <h4>WhatsApp</h4>
              <a href="https://wa.me/62<%= kontak.noTelp %>"
                >+62 <%= kontak.noTelp %></a
              >
            </div>
          </div>
        </div>
        <div class="col-md-6">
          <img
            src="https://i.ibb.co/xD6tRgb/Capture.png"
            alt="test"
            class="img-fluid img-about"
            style="width: 450px"
          />
        </div>
      </div>
    </div>
